:false,"topic_id":3920,"topic_slug":"build-support-for-android-and-webassembly","display_username":"","primary_group_name":null,"flair_name":null,"flair_url":null,"flair_bg_color":null,"flair_color":null,"version":1,"can_edit":false,"can_delete":false,"can_recover":false,"can_wiki":false,"link_counts":[{"url":"https://github.com/rokups/Urho3D/tree/master/Android","internal":false,"reflection":false,"title":"Urho3D/Android at master · rokups/Urho3D · GitHub","clicks":15}],"read":true,"user_title":null,"reply_to_user":{"username":"gunbolt","name":"","avatar_template":"/user_avatar/discourse.urho3d.io/gunbolt/{size}/807_2.png"},"bookmarked":false,"actions_summary":[{"id":2,"count":1}],"moderator":false,"admin":false,"staff":false,"user_id":246,"hidden":false,"trust_level":2,"deleted_at":null,"user_deleted":false,"edit_reason":null,"can_view_edit_history":true,"wiki":false,"can_accept_answer":false,"can_unaccept_answer":false,"accepted_answer":false},{"id":22777,"name":"","username":"gunbolt","avatar_template":"/user_avatar/discourse.urho3d.io/gunbolt/{size}/807_2.png","created_at":"2018-01-07T12:15:54.549Z","cooked":"\u003cp\u003eNot useful because it doesn’t work right out the box with latest Android Studio. There needs to be a proper official support for the latest Android studio and latest NDK release.\u003c/p\u003e","post_number":3,"post_type":1,"updated_at":"2018-01-07T13:01:48.962Z","reply_count":0,"reply_to_post_number":2,"quote_count":0,"incoming_link_count":0,"reads":45,"readers_count":44,"score":9.0,"yours":false,"topic_id":3920,"topic_slug":"build-support-for-android-and-webassembly","display_username":"","primary_group_name":null,"flair_name":null,"flair_url":null,"flair_bg_color":null,"flair_color":null,"version":1,"can_edit":false,"can_delete":false,"can_recover":false,"can_wiki":false,"read":true,"user_title":null,"reply_to_user":{"username":"rku","name":"","avatar_template":"/user_avatar/discourse.urho3d.io/rku/{size}/3200_2.png"},"bookmarked":false,"ac