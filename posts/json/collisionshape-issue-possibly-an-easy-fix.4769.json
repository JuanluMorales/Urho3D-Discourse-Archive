_post_number":null,"quote_count":0,"incoming_link_count":1,"reads":23,"readers_count":22,"score":9.6,"yours":false,"topic_id":4769,"topic_slug":"collisionshape-issue-possibly-an-easy-fix","display_username":"Jonathan","primary_group_name":null,"flair_name":null,"flair_url":null,"flair_bg_color":null,"flair_color":null,"version":1,"can_edit":false,"can_delete":false,"can_recover":false,"can_wiki":false,"read":true,"user_title":null,"bookmarked":false,"actions_summary":[],"moderator":false,"admin":false,"staff":false,"user_id":669,"hidden":false,"trust_level":2,"deleted_at":null,"user_deleted":false,"edit_reason":null,"can_view_edit_history":true,"wiki":false,"can_accept_answer":false,"can_unaccept_answer":false,"accepted_answer":false},{"id":27039,"name":"Christopher Ross","username":"mrchrissross","avatar_template":"https://avatars.discourse-cdn.com/v4/letter/m/58f4c7/{size}.png","created_at":"2018-12-21T08:01:29.152Z","cooked":"\u003cp\u003eDo you mean this type of debug below?\u003c/p\u003e\n\u003cpre\u003e\u003ccode\u003e#include \u0026lt;Urho3D/DebugNew.h\u0026gt;\n    if (drawDebug)\n\t\trenderer-\u0026gt;DrawDebugGeometry(true);\n\u003c/code\u003e\u003c/pre\u003e\n\u003cp\u003eother than that the only debug build that I use is the visual studio one.\u003c/p\u003e","post_number":10,"post_type":1,"updated_at":"2018-12-21T08:02:44.566Z","reply_count":1,"reply_to_post_number":null,"quote_count":0,"incoming_link_count":0,"reads":22,"readers_count":21,"score":9.4,"yours":false,"topic_id":4769,"topic_slug":"collisionshape-issue-possibly-an-easy-fix","display_username":"Christopher Ross","primary_group_name":null,"flair_name":null,"flair_url":null,"flair_bg_color":null,"flair_color":null,"version":2,"can_edit":false,"can_delete":false,"can_recover":false,"can_wiki":false,"read":true,"user_title":null,"bookmarked":false,"actions_summary":[],"moderator":false,"admin":false,"staff":false,"user_id":1061,"hidden":false,"trust_level":1,"deleted_at":null,"user_deleted":false,"edit_reason":null,"can_view_edit_history":true,"wiki":false,"can_accept_answer":false,"can_unaccept_answer":false,"accepted_answer":false},{"id":27040,"name":"魔大农 𝞍𝞎𝝳 現招蜍","username":"Modanung","avatar_template":"/user_avatar/discourse.urho3d.io/modanung/{size}/3290_2.png","created_at":"2018-12-21T11:08:17.107Z","cooked":"\u003caside class=\"quote no-group\" data-username=\"mrchrissross\" data-post=\"10\" data-topic=\"4769\"\u003e\n\u003cdiv class=\"title\"\u003e\n\u003cdiv class=\"quote-controls\"\u003e\u003c/div\u003e\n\u003cimg alt=\"\" width=\"20\" height=\"20\" src=\"https://avatars.discourse-cdn.com/v4/letter/m/58f4c7/40.png\" class=\"avatar\"\u003e mrchrissross:\u003c/div\u003e\n\u003cblockquote\u003e\n\u003cp\u003eother than that the only debug build that I use is the visual studio one.\u003c/p\u003e\n\u003c/blockquote\u003e\n\u003c/aside\u003e\n\u003cp\u003eI think that’s the one \u003ca class=\"mention\" href=\"/u/sinoid\"\u003e@Sinoid\u003c/a\u003e meant.\u003c/p\u003e","post_number":11,"post_type":1,"updated_at":"2018-12-21T11:08:17.107Z","reply_count":1,"reply_to_post_number":10,"quote_count":1,"incoming_link_count":1,"reads":22,"readers_count":21,"score":14.4,"yours":false,"topic_id":4769,"topic_slug":"collisionshape-issue-possibly-an-easy-fix","display_username":"魔大农 𝞍𝞎𝝳 現招蜍","primary_group_name":null,"flair_name":null,"flair_url":null,"flair_bg_color":null,"flair_color":null,"version":1,"can_edit":false,"can_delete":false,"can_recover":false,"can_wiki":false,"read":true,"user_title":"","bookmarked":false,"actions_summary":[],"moderator":false,"admin":false,"staff":false,"user_id":192,"hidden":false,"trust_level":0,"deleted_at":null,"user_deleted":false,"edit_reason":null,"can_view_edit_history":true,"wiki":false,"user_suspended":true,"can_accept_answer":false,"can_unaccept_answer":false,"accepted_answer":false},{"id":27041,"name":"Christopher Ross","username":"mrchrissross","avatar_template":"https://avatars.discourse-cdn.com/v4/letter/m/58f4c7/{size}.png","created_at":"2018-12-21T11:33:15.627Z","cooked":"\u003cp\u003eAh alright so it’s visual studio debugger. Is there anyway to speed it up? Or should I just open with the exe?\u003c/p\u003e","post_number":12,"post_type":1,"updated_at":"2018-12-21T11:33:15.627Z","reply_count":1,"reply_to_post_number":11,"quote_count":0,"incoming_link_count":0,"reads":22,"readers_count":21,"score":9.4,"yours":false,"topic_id":4769,"topic_slug":"collisionshape-issue-possibly-an-easy-fix","display_username":"Christopher Ross","primary_group_name":null,"flair_name":null,"flair_url":null,"flair_bg_color":null,"flair_color":null,"version":1,"can_edit":false,"can_delete":false,"can_recover":false,"can_wiki":false,"read":true,"user_title":null,"reply_to_user":{"username":"Modanung","name":"魔大农 𝞍𝞎𝝳 現招蜍","avatar_template":"/user_avatar/discourse.urho3d.io/modanung/{size}/3290_2.png"},"bookmarked":false,"actions_summary":[],"moderator":false,"admin":false,"staff":false,"user_id":1061,"hidden":false,"trust_level":1,"deleted_at":null,"user_deleted":false,"edit_reason":null,"can_view_edit_history":true,"wiki":false,"can_accept_answer":false,"can_unaccept_answer":false,"accepted_answer":false},{"id":27042,"name":"Jonathan","username":"Sinoid","avatar_template":"https://avatars.discourse-cdn.com/v4/letter/s/f19dbf/{size}.png","created_at":"2018-12-21T19:26:15.393Z","cooked":"\u003cp\u003eOnly use release builds for checking performance - the difference is usually large. In VS you can run as \u003ccode\u003eDebug -\u0026gt; Start without Debugging\u003c/code\u003e (shift+F5 default) to launch from VS without hooking - mostly no different than launching from the executable itself (\u003cem\u003emostly\u003c/em\u003e, still honors VS launch parameters).\u003c/p\u003e\n\u003cp\u003eThe debugger has to \u003cem\u003epatch\u003c/em\u003e things out in order for it to even work (alloc, grabbing stacks, etc) so once you hook you’ve got overhead just for that regardless of whether you’re running Debug/Release.\u003c/p\u003e\n\u003cp\u003eAlthough there’s a profiler in Urho3D it’s just a ballpark tool, you still want to use VS’ \u003ccode\u003eDebug -\u0026gt; Performance Profiler\u003c/code\u003e to find where things are slowing down, which is also only reliable in a Release build.\u003c/p\u003e","post_number":13,"post_type":1,"updated_at":"2018-12-21T19:26:15.393Z","reply_count":0,"reply_to_post_number":12,"quote_count":0,"incoming_link_count":0,"reads":22,"readers_count":21,"score":19.4,"yours":false,"topic_id":4769,"topic_slug":"collisionshape-issue-possibly-an-easy-fix","display_username":"Jonathan","primary_group_name":null,"flair_name":null,"flair_url":null,"flair_bg_color":null,"flair_color":null,"version":1,"can_edit":false,"can_delete":false,"can_recover":false,"can_wiki":false,"read":true,"user_title":null,"reply_to_user":{"username":"mrchrissross","name":"Christopher Ross","avatar_template":"https://avatars.discourse-cdn.com/v4/letter/m/58f4c7/{size}.png"},"bookmarked":false,"actions_summary":[{"id":2,"count":1}],"moderator":false,"admin":false,"staff":false,"user_id":669,"hidden":false,"trust_level":2,"deleted_at":null,"user_deleted":false,"edit_reason":null,"can_view_edit_history":true,"wiki":false,"can_accept_answer":false,"can_unaccept_answer":false,"accepted_answer":false},{"id":27059,"name":"","username":"GodMan","avatar_template":"https://avatars.discourse-cdn.com/v4/letter/g/e79b87/{size}.png","created_at":"2018-12-22T23:49:52.932Z","cooked":"\u003cp\u003eAlso one thing to note is this. If your using your render model for the physics make sure its a simple model. If you use a model that has quite a few polygons then your game will lag bad due to the physics engine having to run calculations on your entire mesh. Most modern games use a very simple representation of the render model that is not rendered in the game “obviously”.\u003c/p\u003e","post_number":14,"post_type":1,"updated_at":"2018-12-22T23:49:52.932Z","reply_count":0,"reply_to_post_nu