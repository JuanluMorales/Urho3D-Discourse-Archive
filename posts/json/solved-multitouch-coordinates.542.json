count":0,"incoming_link_count":0,"reads":7,"readers_count":6,"score":1.4,"yours":false,"topic_id":542,"topic_slug":"solved-multitouch-coordinates","display_username":"","primary_group_name":null,"flair_name":null,"flair_url":null,"flair_bg_color":null,"flair_color":null,"version":1,"can_edit":false,"can_delete":false,"can_recover":false,"can_wiki":false,"read":true,"user_title":null,"bookmarked":false,"actions_summary":[],"moderator":false,"admin":false,"staff":false,"user_id":151,"hidden":false,"trust_level":2,"deleted_at":null,"user_deleted":false,"edit_reason":null,"can_view_edit_history":true,"wiki":false,"can_accept_answer":false,"can_unaccept_answer":false,"accepted_answer":false},{"id":3046,"name":"hd_","username":"hdunderscore","avatar_template":"/user_avatar/discourse.urho3d.io/hdunderscore/{size}/1151_2.png","created_at":"2014-11-11T13:53:06.000Z","cooked":"\u003cp\u003eIt looks like they are both available in TouchState (absolute: position_ / lastPosition_, relative: delta_).\u003c/p\u003e","post_number":4,"post_type":1,"updated_at":"2017-01-02T01:01:16.504Z","reply_count":0,"reply_to_post_number":null,"quote_count":0,"incoming_link_count":19,"reads":7,"readers_count":6,"score":96.4,"yours":false,"topic_id":542,"topic_slug":"solved-multitouch-coordinates","display_username":"hd_","primary_group_name":null,"flair_name":null,"flair_url":null,"flair_bg_color":null,"flair_color":null,"version":1,"can_edit":false,"can_delete":false,"can_recover":false,"can_wiki":false,"read":true,"user_title":"Contributor","title_is_group":false,"bookmarked":false,"actions_summary":[],"moderator":false,"admin":false,"staff":false,"user_id":114,"hidden":false,"trust_level":2,"deleted_at":null,"user_deleted":false,"edit_reason":null,"can_view_edit_history":true,"wiki":false,"can_accept_answer":false,"can_unaccept_answer":false,"accepted_answer":false},{"id":3060,"name":"","username":"sabotage3d","avatar_template":"/user_avatar/discourse.urho3d.io/sabotage3d/{size}/87_2.png","created_at":"2014-11-12T21:21:05.000Z","cooked":"\u003cp\u003ethanks it works quite well\u003cbr\u003e\nJust one thing if we want to check for a touch event is that the best way to do it :\u003c/p\u003e\n\u003cp\u003e[code]bool touchEvent = false;\u003c/p\u003e\n\u003cp\u003eif(input-\u0026gt;GetNumTouches()\u0026gt;0)\u003cbr\u003e\n{\u003cbr\u003e\ntouchEvent = true ;\u003cbr\u003e\n}[/code]\u003c/p\u003e\n\u003cp\u003eI wonder if there is something similar to SDL:\u003c/p\u003e\n\u003cp\u003e\u003ccode\u003eif (evt.type == SDL_FINGERDOWN) {\n    SDL_Touch *state = SDL_GetTouch(evt.tfinger.touchId);\n}\u003c/code\u003e\u003c/p\u003e","post_number":5,"post_type":1,"updated_at":"2017-01-02T01:01:17.528Z","reply_count":0,"reply_to_post_number":null,"quote_count":0,"incoming_link_count":7,"reads":6,"readers_count":5,"score":36.2,"yours":false,"topic_id":542,"topic_slug":"solved-multitouch-coordinates","display_username":"","primary_group_name":null,"flair_name":null,"flair_url":null,"flair_bg_color":null,"flair_color":null,"version":1,"can_edit":false,"can_delete":false,"can_recover":false,"can_wiki":false,"read":true,"user_title":null,"bookmarked":false,"actions_summary":[],"moderator":false,"admin":false,"staff":false,"user_id":151,"hidden":false,"trust_level":2,"deleted_at":null,"user_deleted":false,"edit_reason":null,"can_view_edit_history":true,"wiki":false,"can_accept_answer":false,"can_unaccept_answer":false,"accepted_answer":false},{"id":3064,"name":"hd_","username":"hdunderscore","avatar_template":"/user_avatar/discourse.urho3d.io/hdunderscore/{size}/1151_2.png","created_at":"2014-11-13T03:29:14.000Z","cooked":"\u003cp\u003eThat way works, but if you want another method you can subscribe to the touch events:\u003cbr\u003e\nTouchBegin\u003cbr\u003e\nTouchMove\u003cbr\u003e\nTouchEnd\u003c/p\u003e\n\u003cp\u003eeg:\u003c/p\u003e\n\u003cp\u003e[code]SubscribeToEvent(“TouchBegin”, “Handle_TouchBegin”);\u003cbr\u003e\n}\u003c/p\u003e\n\u003cp\u003evoid Handle_TouchBegin(StringHash eventType, VariantMap\u0026amp; eventData)\u003cbr\u003e\n{\u003cbr\u003e\n\u003ca href=\"http://log.Info\" rel=\"nofollow noopener\"\u003elog.Info\u003c/a\u003e(“TouchBegin”);\u003cbr\u003e\n}[/code]\u003c/p\u003e\n\u003cp\u003e\u003ca href=\"http://urho3d.github.io/documentation/HEAD/_event_list.html\" rel=\"nofollow noopener\"\u003eurho3d.github.io/documentation/H … _list.html\u003c/a\u003e\u003c/p\u003e","post_number":6,"post_type":1,"updated_at":"2017-01-02T01:01:17.878Z","reply_count":0,"reply_to_post_number":null,"quote_count":0,"incoming_link_count":0,"reads":5,"readers_count":4,"score":1.0,"yours":false,"topic_id":542,"topic_slug":"solved-multitouch-coordinates","display_username":"hd_","primary_group_name":null,"flair_name":null,"flair_url":null,"flair_bg_color":null,"flair_color":null,"version":1,"can_edit":false,"can_delete":false,"can_recover":false,"can_wiki":false,"link_counts":[{"url":"http://urho3d.github.io/documentation/HEAD/_event_list.html","internal":false,"reflection":false,"title":"Urho3D - Documentation - Event list","clicks":1},{"url":"http://log.Info","internal":false,"reflection":false,"clicks":0}],"read":true,"user_title":"Contributor","title_is_group":false,"bookmarked":false,"actions_summary":[],"moderator":false,"admin":false,"staff":false,"user_id":114,"hidden":false,"trust_level":2,"deleted_at":null,"user_deleted":false,"edit_reason":null,"can_view_edit_history":true,"wiki":false,"can_accept_answer":false,"can_unaccept_answer":false,"accepted_answer":false},{"id":3072,"name":"","username":"sabotage3d","avatar_template":"/user_avatar/discourse.urho3d.io/sabotage3d/{size}/87_2.png","created_at":"2014-11-13T09:55:15.000Z","cooked":"\u003cp\u003eAwesome works like a charm \u003cimg src=\"https://emoji.discourse-cdn.com/twitter/slight_smile.png?v=5\" title=\":slight_smile:\" class=\"emoji\" alt=\":slight_smile:\"\u003e\u003c/p\u003e","post_number":7,"post_type":1,"updated_at":"2017-01-02T01:01:18.478Z","reply_count":0,"rep