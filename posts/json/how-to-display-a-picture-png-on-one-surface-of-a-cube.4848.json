hnique name=\"Techniques/Diff.xml\" /\u0026gt;\n    \u0026lt;texture unit=\"diffuse\" name=\"YourPath/YourTexture.png\" /\u0026gt;\n\u0026lt;/material\u0026gt;\u003c/code\u003e\u003c/pre\u003e","post_number":7,"post_type":1,"updated_at":"2019-01-23T02:45:30.023Z","reply_count":1,"reply_to_post_number":5,"quote_count":0,"incoming_link_count":0,"reads":27,"readers_count":26,"score":10.4,"yours":false,"topic_id":4848,"topic_slug":"how-to-display-a-picture-png-on-one-surface-of-a-cube","display_username":"","primary_group_name":null,"flair_name":null,"flair_url":null,"flair_bg_color":null,"flair_color":null,"version":1,"can_edit":false,"can_delete":false,"can_recover":false,"can_wiki":false,"read":true,"user_title":null,"reply_to_user":{"username":"majhong","name":"","avatar_template":"/user_avatar/discourse.urho3d.io/majhong/{size}/3577_2.png"},"bookmarked":false,"actions_summary":[],"moderator":false,"admin":false,"staff":false,"user_id":247,"hidden":false,"trust_level":2,"deleted_at":null,"user_deleted":false,"edit_reason":null,"can_view_edit_history":true,"wiki":false,"can_accept_answer":false,"can_unaccept_answer":false,"accepted_answer":false},{"id":27565,"name":"","username":"majhong","avatar_template":"/user_avatar/discourse.urho3d.io/majhong/{size}/3577_2.png","created_at":"2019-01-23T03:25:21.573Z","cooked":"\u003cp\u003eit is not work!\u003c/p\u003e\n\u003cp\u003emy code is:\u003cbr\u003e\n//test load dynamic\u003cbr\u003e\nboxObject-\u0026gt;SetModel(cache-\u0026gt;GetResource\u0026lt;Model\u0026gt;(‚ÄúModels/majhong/mj.mdl‚Äù));\u003cbr\u003e\nboxObject-\u0026gt;SetMaterial(1,cache-\u0026gt;GetResource\u0026lt;Material\u0026gt;(‚ÄúModels/majhong/mj_1.xml‚Äù));\u003cbr\u003e\nboxObject-\u0026gt;SetMaterial(2,cache-\u0026gt;GetResource\u0026lt;Material\u0026gt;(‚ÄúModels/majhong/mj_2.xml‚Äù));\u003cbr\u003e\n//load face\u003cbr\u003e\nboxObject-\u0026gt;SetMaterial(0,cache-\u0026gt;GetResource\u0026lt;Material\u0026gt;(‚ÄúModels/majhong/mj_0.xml‚Äù));\u003cbr